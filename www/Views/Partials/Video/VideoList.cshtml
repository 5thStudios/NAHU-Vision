@inherits Umbraco.Web.Mvc.UmbracoViewPage<List<NAHUvision.Models.Video>>


@{ 
    string searchResultClass = NAHUvision.Models.Common.Miscellaneous.SearchResult;
}


    <div id="videoBlocks" class="grid-x grid-padding-x grid-padding-y small-up-1 medium-up-2 large-up-4 ">
        @foreach (NAHUvision.Models.Video video in Model)
        {
            Html.RenderPartial("~/Views/Partials/Video/VideoBlock.cshtml", video);
        }
        <input type="hidden" id="hfldSearchResultClass" value="@searchResultClass" />
    </div>